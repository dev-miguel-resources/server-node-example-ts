{
   "name": "server-example",
   "version": "1.0.0",
   "description": "Node Server Example",
   "main": "index.js",
   "scripts": {
      "start": "node index.js",
      "start:ts": "ts-node src/index.ts",
      "dev": "nodemon src/index.ts",
      "format": "prettier --ignore-path .gitignore node_modules --write .",
      "lint": "eslint . --ext .{js,ts,tsx}",
      "lint.fix": "npx eslint --fix ."
   },
   "keywords": [],
   "author": "Miguel Chamorro",
   "license": "ISC",
   "dependencies": {
      "express": "4.18.2",
      "ts-node": "10.9.1"
   },
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "5.44.0",
      "@typescript-eslint/parser": "5.44.0",
      "eslint": "8.28.0",
      "jest": "29.3.1",
      "nodemon": "2.0.20",
      "prettier": "2.8.0",
      "pretty-quick": "3.1.3",
      "simple-git-hooks": "2.8.1",
      "typescript": "4.9.3"
   },
   "simple-git-hooks": {
      "pre-commit": "npx pretty-quick --staged"
   }
}
